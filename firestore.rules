
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /users/{userId} {
      allow read, write: if request.auth.uid == userId;
      allow list: if request.auth != null;
    }

    match /contestSessions/{sessionId} {
      allow read, write: if request.auth != null;
    }
  }
}
